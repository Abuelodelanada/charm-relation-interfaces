{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://github.com/canonical/charm-relation-interfaces/ingress_per_unit/schemas/provider.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
          "data": {
            "urls": {
              "unit_name": "http://foo.bar:80/model_name-unit_name/0"
            }
          }
        }
    ],
    "required": [
        "data"
    ],
    "properties": {
        "data": {
            "$id": "#/properties/data",
            "type": "string",
            "title": "yaml-encoded unit urls",
            "description": "url",
            "default": null,
            "examples": [
                {
                    "urls": {
                        "unit_name": "http://foo.bar:80/model_name-unit_name/0"
                    }
                }
            ]
        }
    },
    "additionalProperties": false
}
