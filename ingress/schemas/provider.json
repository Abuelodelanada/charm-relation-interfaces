{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://github.com/canonical/charm-relation-interfaces/ingress/schemas/provider.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "url": "http://foo.bar:80/model_name-app_name"
        }
    ],
    "required": [
        "url"
    ],
    "properties": {
        "url": {
            "$id": "#/properties/url",
            "type": "string",
            "title": "application url",
            "description": "The ingress url for the application",
            "default": null,
            "examples": [
                "http://foo.bar:80/prom-model-prometheus-k8s"
            ]
        }
    },
    "additionalProperties": false
}
